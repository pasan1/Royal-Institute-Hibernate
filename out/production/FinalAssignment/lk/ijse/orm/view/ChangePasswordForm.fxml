<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="window" prefHeight="669.0" prefWidth="715.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.orm.controller.ChangePasswordFormController">
   <children>
      <TextField fx:id="txtOldPassword" layoutX="332.0" layoutY="99.0" prefHeight="31.0" prefWidth="249.0" promptText="Enter Old Password" />
      <Text layoutX="135.0" layoutY="120.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Old Password :" />
      <Text layoutX="135.0" layoutY="172.0" strokeType="OUTSIDE" strokeWidth="0.0" text="New Password :" />
      <Text layoutX="135.0" layoutY="232.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Confirm Password :" />
      <TextField fx:id="txtNewPassword" layoutX="332.0" layoutY="151.0" prefHeight="31.0" prefWidth="249.0" promptText="Enter New Password" />
      <TextField fx:id="txtReNewPassword" layoutX="332.0" layoutY="211.0" prefHeight="31.0" prefWidth="249.0" promptText="Re-Enter New Password" />
      <AnchorPane fx:id="paneButtons" layoutX="16.0" layoutY="335.0" prefHeight="58.0" prefWidth="684.0">
         <children>
            <JFXButton fx:id="btnExit" layoutX="214.0" layoutY="13.0" onAction="#btnExitOnAction" prefHeight="30.0" prefWidth="100.0" style="-fx-border-color: red;" text="Exit">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </JFXButton>
            <JFXButton fx:id="btnUpdate" layoutX="376.0" layoutY="13.0" onAction="#btnUpdateOnAction" prefHeight="30.0" prefWidth="100.0" style="-fx-border-color: green;" text="Update">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </JFXButton>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
